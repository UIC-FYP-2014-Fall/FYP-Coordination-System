<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
  <form-beans >
    <form-bean name="userForm" type="com.uic.web.struts.form.UserForm" />

  </form-beans>

  <global-exceptions />
  <global-forwards >
    <forward name="goLogin" path="/index.jsp" />

  </global-forwards>

  <action-mappings >
    
    <action
      parameter="flag"
      path="/goUI"
      type="com.uic.web.struts.action.GoUIAction"
      validate="false"
      cancellable="true" >
      <forward name="goMainUi" path="/WEB-INF/logined/Coordinator/main.jsp" />
      <forward name="goTeacherMainUI" path="/WEB-INF/logined/Teacher/teacherMain.jsp" />
    </action>
    <action
      input="/WEB-INF/logined/Coordinator/main.jsp"
      parameter="flag"
      path="/goManageAccountUi"
      type="com.uic.web.struts.action.GoManageAccountUiAction"
      validate="false"
      cancellable="true" >
      <forward name="goManageAccountUi" path="/WEB-INF/logined/Coordinator/manageAccount.jsp" />
    </action>
    <action
      attribute="userForm"
      input="/WEB-INF/login.jsp"
      name="userForm"
      path="/login"
      scope="request"
      type="com.uic.web.struts.action.LoginAction"
      validate="false"
      cancellable="true">
      <forward name="loginok" path="/goUI.do" />
      <forward name="goLoginUi" path="/WEB-INF/login.jsp" />
    </action>
    <action
      path="/logout"
      type="com.uic.web.struts.action.LogoutAction"
      validate="false"
      cancellable="true" >
      <forward name="goLoginUi" path="/WEB-INF/login.jsp" />
    </action>
    <action
      input="/WEB-INF/logined/Teacher/teacherMain.jsp"
      path="/goUploadTopicUi"
      scope="request"
      validate="false"
      type="com.uic.web.struts.action.GoUploadTopicUiAction"
      cancellable="true" >
      <forward name="goUploadTopicUi" path="/WEB-INF/logined/Teacher/uploadTopic.jsp" />
    </action>
    <action
      path="/goChangePasswordUi"
      scope="request"
      type="com.uic.web.struts.action.GoChangePasswordUiAction"
      validate="false"
      cancellable="true">
      <forward name="goChangePasswordUi" path="/WEB-INF/logined/Teacher/changePwd.jsp" />
    </action>
    <action
      path="/goChooseObserverUi"
      type="com.uic.web.struts.action.GoChooseObserverUiAction"
      cancellable="true">
      <forward name="goChooseObserverUi" path="/WEB-INF/logined/Teacher/chooseObserver.jsp" />
    </action>
    <action
      path="/goEditTopicUi"
      type="com.uic.web.struts.action.GoEditTopicUiAction"
      cancellable="true">
      <forward name="goEditTopicUi" path="/WEB-INF/logined/Teacher/editTopic.jsp" />
    </action>
    <action
      path="/goTimetableUi"
      type="com.uic.web.struts.action.GoTimetableUiAction"
      cancellable="true">
      <forward name="goTimetableUi" path="/WEB-INF/logined/Teacher/timetable.jsp" />
    </action>
    <action
      path="/goTopicListUi"
      type="com.uic.web.struts.action.GoTopicListUiAction"
      cancellable="true">
      <forward name="goTopicListUi" path="/WEB-INF/logined/Teacher/topicList.jsp" />
    </action>
    


    

    




  </action-mappings>

  <message-resources parameter="com.uic.web.struts.ApplicationResources" />
</struts-config>

